<div class="row">
  <%= render "shared/account_nav" %>
  <div class="span10">
    <section class="project-section">
      <fieldset>
        <legend><%= @project.name.upcase %></legend>
        <p><%= @project.description %></p>  
        <%= render "project" %>
      </fieldset>
    </section> 
    <section id="activities" class="project-section">
      <fieldset>
        <legend>Activities</legend> 
        <%= render "log" %>
      </fieldset>
    </section>  
    <section id="charts" class="project-section">
      <fieldset>
        <legend>Charts</legend>
        <div class="div-chart" id="chartset-project-hours"></div>
        <div class="div-chart" id="chartset-tasks-span"></div>
        <div class="div-chart" id="chartset-tasks-hours"></div>
      </fieldset>  
    </section>  
    <section id="tool-box" class="project-section">
      <fieldset>
        <legend>Tool Box</legend>
        <%= render "team" %>
        <%= render "record" %>
      </fieldset>
    </section>
  </div>
</div>
